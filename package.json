{
    "name": "@jullury-fluent/smart-api-server",
    "version": "0.0.16",
    "description": "Server utilities and helpers",
    "keywords": [
        "nestjs",
        "sequelize",
        "utilities",
        "pagination",
        "sort",
        "filter",
        "query"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/jullury-fluent/smart-api-server.git"
    },
    "license": "UNLICENSED",
    "author": "",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./orm": {
            "types": "./dist/orm/index.d.ts",
            "import": "./dist/orm/index.mjs",
            "require": "./dist/orm/index.js"
        }
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsup",
        "clean": "rimraf dist",
        "commit": "git-cz",
        "dev": "tsup --watch",
        "format": "prettier --write \"src/**/*.{ts,js,tsx,jsx,css,scss,html,json,md,mdx,markdown,graphql,prisma}\" && sort-package-json",
        "format:check": "prettier --check \"src/**/*.{ts,js,tsx,jsx,css,scss,html,json,md,mdx,markdown,graphql,prisma}\"",
        "lint": "eslint --ext .ts,.js,.tsx,.jsx src",
        "lint:fix": "eslint --ext .ts,.js,.tsx,.jsx src --fix",
        "prepare": "husky && pnpm setup-git-aliases",
        "prepublishOnly": "pnpm build",
        "semantic-release": "semantic-release",
        "setup-git-aliases": "./setup-git-aliases.sh",
        "sort-package-json": "sort-package-json",
        "test": "jest --detectOpenHandles",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:watch": "jest --watch",
        "preversion": "npm run build"
    },
    "dependencies": {
        "@jullury-fluent/smart-api-common": "^1.0.0",
        "@nestjs/common": "^10.4.20",
        "@nestjs/sequelize": "^10.0.1",
        "@nestjs/swagger": "^7.4.2",
        "@swc/core": "^1.13.3",
        "reflect-metadata": "^0.1.14",
        "sequelize": "^6.37.7",
        "sequelize-typescript": "^2.1.6",
        "zod": "^3.25.76",
        "zod-metadata": "^1.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.23.9",
        "@babel/preset-env": "^7.23.9",
        "@babel/preset-typescript": "^7.23.3",
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.32.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^11.0.3",
        "@semantic-release/npm": "^12.0.2",
        "@swc/core": "^1.13.3",
        "@types/jest": "^29.5.14",
        "@types/react": "^19.1.9",
        "@typescript-eslint/eslint-plugin": "^8.38.0",
        "@typescript-eslint/parser": "^8.38.0",
        "babel-jest": "^29.7.0",
        "commit-and-tag-version": "^12.5.2",
        "commitizen": "^4.3.1",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^9.32.0",
        "globals": "^16.3.0",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^30.0.5",
        "prettier": "^3.6.2",
        "rimraf": "^5.0.5",
        "semantic-release": "^24.2.7",
        "sort-package-json": "^3.4.0",
        "ts-jest": "^29.4.0",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsup": "^8.0.1",
        "typescript": "^5.8.3"
    },
    "publishConfig": {
        "access": "restricted",
        "registry": "https://npm.pkg.github.com/"
    }
}
